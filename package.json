{
    "name": "snippets-backend",
    "version": "1.0.50",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "bun run --watch src/server.ts",
        "db:studio": "bun prisma studio",
        "db:generate": "bun prisma generate",
        "db:migrate": "bun prisma migrate dev",
        "start:prod": "bun prisma migrate deploy && bun start",
        "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/server.ts"
    },
    "dependencies": {
        "@elysiajs/swagger": "^1.1.3",
        "@prisma/client": "5.20.0",
        "elysia": "latest",
        "prisma": "^5.20.0"
    },
    "devDependencies": {
        "bun-types": "latest"
    },
    "module": "src/server.js"
}
